page {
  background: #f9f9f9;
  height: 100%;
}
.bg-img-box image {
  width: 100%;
  height: 240px;
}
.btn-box {
  padding: 50rpx 60rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 150px;
}
.btn-box .list-box {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    padding: 30rpx 40rpx;
    background-color: #fff;
    -webkit-box-shadow: 3px 3px 8px 3px rgba(210, 210, 210, 0.7);
            box-shadow: 3px 3px 8px 3px rgba(210, 210, 210, 0.7);
    border-radius: 10rpx;
    margin-bottom: 40rpx;
    color: #333;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
}
.btn-box .list-box:active {
      background-color: rgba(240, 240, 240, 0.7);
}
.btn-box .list-box image {
      width: 100rpx;
      height: 100rpx;
      margin-right: 30rpx;
}
.btn-box .list-box .title {
      font-size: 36rpx;
      margin-bottom: 5px;
}
.btn-box .list-box .grey {
      color: #666;
}
.b-t-panel .auth-title-box {
  height: 50px;
  line-height: 50px;
}
.b-t-panel .auth-title-box .left-box image {
    width: 20px;
    height: 20px;
    position: relative;
    top: 3px;
}
.b-t-panel .auth-title-box .left-box .title {
    font-size: 32rpx;
    font-weight: 500;
    margin: 0 10px;
}
.b-t-panel .auth-content-box {
  padding: 20px 0;
}
.b-t-panel .auth-btn-box {
  margin-top: 20px;
}
.b-t-panel .auth-btn-box view {
    width: 47%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    border-radius: 5px;
    font-size: 16px;
}
.b-t-panel .auth-btn-box .btn-cancel {
    background-color: rgba(230, 230, 230, 0.7);
    color: #39B54A;
}
.b-t-panel .auth-btn-box .btn-cancel:active {
      background-color: rgba(210, 210, 210, 0.7);
}
.b-t-panel .auth-btn-box .btn-sure {
    background-color: #39B54A;
    color: #fff;
}
.b-t-panel .auth-btn-box .btn-sure:active {
      background-color: rgba(50, 150, 57, 0.7);
}

page::after{position:fixed;content:'';left:-1000px;top:-1000px;-webkit-animation:shadow-preload .1s;-webkit-animation-delay:3s;animation:shadow-preload .1s;animation-delay:3s}@-webkit-keyframes shadow-preload{0%{background-image:url(https://cdn.dcloud.net.cn/img/shadow-grey.png)}100%{background-image:url(https://cdn.dcloud.net.cn/img/shadow-grey.png)}}@keyframes shadow-preload{0%{background-image:url(https://cdn.dcloud.net.cn/img/shadow-grey.png)}100%{background-image:url(https://cdn.dcloud.net.cn/img/shadow-grey.png)}}