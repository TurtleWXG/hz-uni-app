page {
  background: #fff;
  height: 100%;
}
page .search-box {
    padding: 10px 15px;
    background: #EFEFEF;
}
page .search-box input {
      height: 32px;
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      background: #E0E0E0;
      border-radius: 6rpx;
      padding: 0 10px;
}
page .examine-box .examine-list {
    padding: 20rpx 40rpx;
    border-bottom: 1px solid #eee;
}
page .examine-box .examine-list:active {
      background: #eee;
}
page .examine-box .examine-list:last-child {
      margin-bottom: 100px;
}
page .examine-box .examine-list .title {
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
          -ms-flex-pack: justify;
              justify-content: space-between;
      -webkit-box-align: end;
      -webkit-align-items: flex-end;
          -ms-flex-align: end;
              align-items: flex-end;
      margin-bottom: 10px;
}
page .examine-box .examine-list .title .name {
        font-size: 36rpx;
}
page .examine-box .examine-list .title view {
        color: #999;
}
page .examine-box .examine-list .text {
      color: #999;
}
page .scan-box {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    line-height: 50px;
    padding-bottom: 10px;
    color: #fff;
    -webkit-box-shadow: 3px 3px 4px rgba(0, 102, 204, 0.2);
            box-shadow: 3px 3px 4px rgba(0, 102, 204, 0.2);
}
page .scan-box view {
      height: 100%;
      width: 100%;
      text-align: center;
      font-size: 32rpx;
}
page .scan-box view image {
        width: 20px;
        height: 20px;
        position: relative;
        top: 4px;
        left: -10px;
}
page .none-data {
    text-align: center;
    margin-top: 100px;
}

