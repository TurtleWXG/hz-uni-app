<view class="container"><scroll-view style="height:100vh;" scroll-y><view class="search-box flex justify-between"><input type="text" value="" placeholder="姓名/身份证号" data-event-opts="{{[['input',[['updateInputChange',['$event']]]]]}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['formSubmit',['$event']]]]]}}" class="cu-btn bg-blue shadow" catchtap="__e">查询</button></view><view class="page-box"><block wx:if="{{examineList.length>0}}"><uni-pagination vue-id="1" title="标题文字" pageSize="{{pageSize}}" total="{{total}}" current="{{currentPage}}" canClick="{{canFetch}}" data-event-opts="{{[['^change',[['jumpPage']]]]}}" bind:change="__e" bind:__l="__l"></uni-pagination></block></view><scroll-view class="examine-box" style="height:calc(100vh - 93px);" scroll-y><block wx:if="{{examineList.length>0}}"><view><block wx:for="{{$root.l0}}" wx:for-item="items" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['navToPage',['examine/examine-list-detail?id='+items.$orig.id+'&flag=false']]]]]}}" class="examine-list" bindtap="__e"><view class="title"><text class="name">{{items.$orig.xm+''}}<block wx:if="{{items.$orig.zjhmRed}}"><text class="{{[items.$orig.zjhmRed?'red':'']}}">(湖北籍)</text></block></text><view><text>{{items.f0}}</text></view></view><text class="{{['text',items.$orig.zjhmRed?'red':'']}}">{{items.f1}}</text></view></block></view></block><block wx:else><view class="none-data">无数据</view></block></scroll-view><view class="scan-box bg-blue"><view data-event-opts="{{[['tap',[['scanInfo',['$event']]]]]}}" catchtap="__e"><image src="../../static/sancode.png" mode></image>扫码审核</view></view></scroll-view><message class="vue-ref" vue-id="2" duration="{{2000}}" background data-ref="Message" bind:__l="__l"></message></view>