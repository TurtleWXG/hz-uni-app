page {
  background: #fff;
  height: 100%;
}
page .container {
    height: 100%;
}
page .record-box {
    padding-bottom: 60px;
}
page .record-list {
    padding: 20rpx 40rpx;
    border-bottom: 1px solid #eee;
}
page .record-list:active {
      background: #eee;
}
page .record-list:last-child {
      margin-bottom: 200px;
}
page .record-list .title {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      -webkit-box-align: end;
      -webkit-align-items: flex-end;
              align-items: flex-end;
      margin-bottom: 10px;
}
page .record-list .title .name {
        font-size: 36rpx;
}
page .record-list .title view {
        color: #999;
}
page .record-list .title .text {
        color: #999;
}
page .record-list .title .mr {
        margin-right: 5px;
}
page .none-data {
    text-align: center;
    margin-top: 100px;
}

