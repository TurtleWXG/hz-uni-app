page {
  background: #fff;
  height: 100%;
}
page .person-info {
    border-bottom: 1px solid #eee;
}
page .info {
    padding: 40rpx 30rpx;
}
page .info .title {
      font-size: 36rpx;
}
page .info .row-list {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      padding-top: 10px;
      -webkit-box-align: baseline;
      -webkit-align-items: baseline;
              align-items: baseline;
}
page .info .row-list .label {
        min-width: calc(4em + 15px);
        color: #666;
}
page .btn-box {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    height: 56px;
    background: #eee;
    padding: 0 10%;
}
page .btn-box button {
      width: 40%;
}
page .fuzhi {
    font-size: 30rpx;
    margin-left: 15rpx;
}
